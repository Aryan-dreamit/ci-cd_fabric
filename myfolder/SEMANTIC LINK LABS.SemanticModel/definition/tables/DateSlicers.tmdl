	partition DateSlicers-3b287c10-e349-42cf-adfe-9ad36cc93f65 = calculated
		mode: import
		source =
				
				VAR TodayDate = TODAY()
				VAR YearStart = DATE(YEAR(TodayDate),1,1)
				VAR MonthStart = DATE(YEAR(TodayDate),MONTH(TodayDate),1)
				VAR Result = UNION(ADDCOLUMNS(CALENDAR(YearStart,TodayDate),"Selection","YTD","Order",7),ADDCOLUMNS(CALENDAR(MonthStart,TodayDate),"Selection","Current Week","Order",1),ADDCOLUMNS(CALENDAR(TodayDate-7,TodayDate-1),"Selection","Last Week","Order",2),ADDCOLUMNS(CALENDAR(TodayDate-28,TodayDate),"Selection","Last 4 Weeks","Order",3),ADDCOLUMNS(CALENDAR(TodayDate-91,TodayDate),"Selection","Last 13 Weeks","Order",4),ADDCOLUMNS(CALENDAR(TodayDate-365,TodayDate),"Selection","Last 52 Weeks","Order",5))
				RETURN
				ALLNOBLANKROW('Date Slicer Table')//Result

	annotation PBI_Id = b289109532874ca3b5c804fd4d90dfe0

