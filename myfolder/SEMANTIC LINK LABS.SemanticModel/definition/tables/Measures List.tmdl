			VAR _Count=DISTINCTCOUNT('WMT Weeks'[WMT WK Number])
			VAR _SelectedWeeks =If(_Count  <= 10 ,CONCATENATEX( VALUES('WMT Weeks'[WMT WK Number] ), 'WMT Weeks'[WMT WK Number],", "), "Multiple")
			
			RETURN IF(CONTAINSSTRING(MAX('Calculationby?'[Calculationby?]),"Last"), _lastNweeks,MAX('Calculationby?'[Calculationby?]) & ": "  &   If(MAX('WMT Weeks'[WMT WK Number]) ==  _Count  ,"All",_SelectedWeeks))
